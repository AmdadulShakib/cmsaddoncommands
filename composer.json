{
  "name": "amdadulshakib/cmsaddoncommands",
  "description": "A set of reusable Laravel Artisan commands for CMS-based projects.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "AmdadulShakib",
      "email": "amdadulshakib@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.0",
    "illuminate/support": "^8.0|^9.0|^10.0|^11.0|^12.0"
  },
  "autoload": {
    "psr-4": {
      "CMSAddonCommands\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "CMSAddonCommands\\CMSAddonCommandServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}